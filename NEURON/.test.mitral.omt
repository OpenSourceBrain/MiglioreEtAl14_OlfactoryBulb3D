# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation 

target: mitralTest.py 
engine: PyNEURON
mep: .test.MitralCell.spikes.mep
experiments:
  spikes4:
    observables:
      spike times:
        file: 
          path: Mitral_1nA.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0.0 

<!-- This is the minimum LEMS file to get jnml to generate cell .hoc + channel .mod + network .py files -->

<Lems>
	<Include file="Cells.xml"/>
	<Include file="Networks.xml"/>
	<Include file="Simulation.xml"/>

	<Include file="[NetworkFile]" />

	<Target component="sim"/>

	<Simulation id="sim" length="3000ms" step="0.025ms" target="[NetworkID]">
	</Simulation>
</Lems>
